on boot && property:ro.boot.product.hardware.sku=nfc_ss
    start vendor.nfc_hal_service
on boot && property:ro.boot.product.hardware.sku=nfc_dsds
    start vendor.nfc_hal_service

service vendor.nfc_hal_service /vendor/bin/hw/android.hardware.nfc@1.2-service
    class early_hal
    user nfc
    group nfc
    disabled
